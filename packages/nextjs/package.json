{
  "name": "@prezly/theme-kit-nextjs",
  "version": "0.0.0-next-cdfd4c2a8218fedccda0fa9bb036397a33437b89",
  "description": "Data layer and utility library for developing Prezly themes with NextJS",
  "main": "build/index.js",
  "module": "build/index.mjs",
  "types": "build/index.d.ts",
  "files": ["build", "README.md"],
  "imports": {
    "#utils": "./build/utils/index.js"
  },
  "exports": {
    ".": {
      "types": "./build/index.d.ts",
      "import": "./build/index.mjs",
      "default": "./build/index.js"
    },
    "./adapters/client": {
      "types": "./build/adapters/client.d.ts",
      "import": "./build/adapters/client.mjs",
      "default": "./build/adapters/client.js"
    },
    "./adapters/server": {
      "types": "./build/adapters/server.d.ts",
      "import": "./build/adapters/server.mjs",
      "default": "./build/adapters/server.js"
    },
    "./http": {
      "types": "./build/http/index.d.ts",
      "import": "./build/http/index.mjs",
      "default": "./build/http/index.js"
    },
    "./middleware": {
      "types": "./build/middleware/server.d.ts",
      "import": "./build/middleware/server.mjs",
      "default": "./build/middleware/server.js"
    },
    "./next-config": {
      "types": "./build/next-config/index.d.ts",
      "import": "./build/next-config/index.mjs",
      "default": "./build/next-config/index.js"
    },
    "./sentry": {
      "types": "./build/sentry/index.d.ts",
      "import": "./build/sentry/index.mjs",
      "default": "./build/sentry/index.js"
    },
    "./playwright": {
      "types": "./build/playwright/index.d.ts",
      "import": "./build/playwright/index.mjs",
      "default": "./build/playwright/index.js"
    }
  },
  "engines": {
    "node": ">= 16.x",
    "npm": ">= 8.x"
  },
  "devEngines": {
    "node": ">= 16.x",
    "npm": ">= 8.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prezly/theme-kit-js.git",
    "directory": "packages/nextjs"
  },
  "keywords": [
    "prezly",
    "nextjs"
  ],
  "author": "Prezly Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/prezly/theme-kit-js/issues"
  },
  "homepage": "https://github.com/prezly/theme-kit-js/tree/main/packages/nextjs#readme",
  "scripts": {
    "prebuild": "rimraf build/* *.tsbuildinfo",
    "build": "npm run build:types && npm run build:esm && npm run build:cjs",
    "build:types": "tsc --project .",
    "build:cjs": "babel --config-file=../../babel.cjs.js src/ --extensions=.ts,.tsx --out-dir=build/",
    "build:esm": "babel --config-file=../../babel.esm.js src/ --extensions=.ts,.tsx --out-dir=build/ --out-file-extension=.mjs",
    "dev": "tsc --watch --preserveWatchOutput --project .",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false --incremental --project .",
    "lint": "eslint src",
    "test": "npm run test:build",
    "test:build": "node test/test-build.cjs && node test/test-build.mjs"
  },
  "peerDependencies": {
    "@playwright/test": "1.x",
    "@prezly/sdk": "^18.2.0",
    "next": "^12.x || ^13.x",
    "react": "^17.x || ^18.x",
    "react-dom": "^17.x || ^18.x"
  },
  "peerDependenciesMeta": {
    "@playwright/test": {
      "optional": true
    }
  },
  "dependencies": {
    "@e1himself/cached-fetch": "^1.1.2",
    "@prezly/theme-kit-core": "0.0.0-next-cdfd4c2a8218fedccda0fa9bb036397a33437b89",
    "@prezly/theme-kit-intl": "0.0.0-next-cdfd4c2a8218fedccda0fa9bb036397a33437b89",
    "next-seo": "^5.4.0",
    "parse-data-url": "^6.0.0",
    "url-pattern": "^1.0.3"
  },
  "devDependencies": {
    "@sentry/nextjs": "7.80.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false
}

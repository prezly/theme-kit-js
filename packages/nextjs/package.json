{
  "name": "@prezly/theme-kit-nextjs",
  "version": "4.0.0",
  "description": "Data layer and utility library for developing Prezly themes with NextJS",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": ["build", "src"],
  "engines": {
    "node": ">= 16.x",
    "npm": ">= 8.x"
  },
  "devEngines": {
    "node": ">= 16.x",
    "npm": ">= 8.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prezly/theme-kit-js.git",
    "directory": "packages/nextjs"
  },
  "keywords": ["prezly", "nextjs"],
  "author": "Prezly Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/prezly/theme-kit-js/issues"
  },
  "homepage": "https://github.com/prezly/theme-kit-js/tree/main/packages/nextjs#readme",
  "scripts": {
    "prebuild": "rimraf build/* *.tsbuildinfo",
    "build": "tsc --project .",
    "dev": "tsc --watch --preserveWatchOutput --project .",
    "typecheck": "tsc --noEmit --incremental --project .",
    "lint": "eslint src",
    "test": "jest"
  },
  "peerDependencies": {
    "@prezly/theme-kit-core": "^4.x",
    "next": "^12.x || ^13.x",
    "react": "^17.x || ^18.x",
    "react-dom": "^17.x || ^18.x"
  },
  "dependencies": {
    "@prezly/sdk": "^11.11.0",
    "@react-hookz/web": "^16.0.0",
    "next-seo": "^5.4.0"
  },
  "devDependencies": {
    "@sentry/nextjs": "7.47.0",
    "rimraf": "3.0.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false
}

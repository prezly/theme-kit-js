{
    "name": "@prezly/theme-kit-intl",
    "version": "0.0.0-next-745047af18de4df62baba5cc177eb85d7ffe5225",
    "description": "Translations for Prezly themes",
    "main": "build/index.js",
    "module": "build/index.mjs",
    "types": "build/index.d.ts",
    "exports": {
        ".": {
            "types": "./build/index.d.ts",
            "import": "./build/index.mjs",
            "require": "./build/index.js",
            "default": "./build/index.js"
        },
        "./i18n": "./build/i18n",
        "./i18n/*": "./build/i18n/*"
    },
    "files": [
        "build",
        "README.md"
    ],
    "engines": {
        "node": ">= 14.x",
        "npm": ">= 7.x"
    },
    "devEngines": {
        "node": ">= 14.x",
        "npm": ">= 7.x"
    },
    "scripts": {
        "clean": "rimraf build/* *.tsbuildinfo",
        "prebuild": "npm run clean && npm run build:locales && npm run prettier:check",
        "build": "npm run intl:compile && npm run build:types && npm run build:esm && npm run build:cjs",
        "build:locales": "node ./build-locales.js > ./src/locales.ts && prettier --write ./src/locales.ts",
        "build:types": "tsc --project .",
        "build:esm": "babel --config-file=../../babel.esm.js src/ --extensions=.ts --out-dir=build/ --out-file-extension=.mjs",
        "build:cjs": "babel --config-file=../../babel.cjs.js src/ --extensions=.ts --out-dir=build/",
        "watch": "tsc --watch --preserveWatchOutput --project .",
        "lint": "eslint ./src/",
        "typecheck": "tsc --project . --noEmit --emitDeclarationOnly false",
        "prettier:check": "prettier --check './src/**/*.{ts,js}'",
        "prettier:fix": "prettier --write './src/**/*.{ts,js}'",
        "test": "npm run test:unit && npm run test:build",
        "test:unit": "jest",
        "test:build": "npm run test:build:i18n && npm run test:build:i18n:dist && npm run test:build:cjs && npm run test:build:esm && npm run test:build:dts",
        "test:build:dts": "tsc --noEmit build/index.d.ts",
        "test:build:i18n": "node test/test-i18n.mjs",
        "test:build:i18n:dist": "echo 'Checking if the build/i18n directory contains 49 files' && ls build/i18n/*.json | cut -d\\/ -f3 | cut -d. -f1 | sort | wc -l | grep -q 49 || (echo 'Fail' && false) && echo 'OK'",
        "test:build:cjs": "node test/test-build.cjs",
        "test:build:esm": "node test/test-build.mjs",
        "intl:extract": "formatjs extract 'src/messages/**/*.ts' --out-file i18n/en.json --format crowdin --id-interpolation-pattern '[sha512:contenthash:base64:6]'",
        "intl:compile": "formatjs compile-folder --ast --format crowdin i18n build/i18n"
    },
    "np": {
        "yarn": false
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/prezly/theme-kit-js.git"
    },
    "keywords": [],
    "author": "Prezly Team <hello@prezly.com> (https://www.prezly.com)",
    "license": "GNU GPL v3",
    "bugs": {
        "url": "https://github.com/prezly/theme-kit-js/issues"
    },
    "homepage": "https://github.com/prezly/theme-kit-js/tree/main/packages/intl#readme",
    "dependencies": {
        "@formatjs/intl": "^1.16.0",
        "@technically/is-not-undefined": "^1.0.0"
    },
    "devDependencies": {
        "@formatjs/cli": "4.8.4"
    },
    "publishConfig": {
        "access": "public"
    }
}

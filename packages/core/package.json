{
  "name": "@prezly/theme-kit-core",
  "version": "4.3.4",
  "description": "Data layer and utility library for developing Prezly themes with JavaScript",
  "type": "module",
  "main": "build/cjs/index.js",
  "types": "build/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./build/esm/index.js",
      "require": "./build/cjs/index.js",
      "default": "./build/esm/index.js"
    },
    "./localeConfig": {
      "import": "./build/esm/intl/localeConfig.js",
      "require": "./build/cjs/intl/localeConfig.js",
      "default": "./build/esm/intl/localeConfig.js"
    },
    "./localeList": {
      "import": "./build/esm/intl/localeList.js",
      "require": "./build/cjs/intl/localeList.js",
      "default": "./build/esm/intl/localeList.js"
    },
    "./build/intl/localeConfig": {
      "import": "./build/esm/intl/localeConfig.js",
      "require": "./build/cjs/intl/localeConfig.js",
      "default": "./build/esm/intl/localeConfig.js"
    }
  },
  "files": ["build", "README.md"],
  "scripts": {
    "prebuild": "rimraf build/* *.tsbuildinfo",
    "build:esm": "tsc --project tsconfig.esm.json",
    "build:cjs": "tsc --project tsconfig.cjs.json",
    "build": "npm run build:esm && npm run build:cjs",
    "dev": "tsc --watch --preserveWatchOutput --project tsconfig.base.json",
    "typecheck": "tsc --noEmit --incremental --project tsconfig.base.json",
    "lint": "eslint src",
    "test": "jest"
  },
  "dependencies": {
    "@prezly/sdk": "^11.11.0",
    "@prezly/uploadcare": "^2.3.4",
    "parse-data-url": "^4.0.1"
  },
  "devDependencies": {
    "@types/parse-data-url": "3.0.0",
    "rimraf": "3.0.2"
  },
  "engines": {
    "node": ">= 16.x",
    "npm": ">= 8.x"
  },
  "devEngines": {
    "node": ">= 16.x",
    "npm": ">= 8.x"
  },
  "keywords": ["prezly"],
  "author": "Prezly Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/prezly/theme-kit-js/issues"
  },
  "homepage": "https://github.com/prezly/theme-kit-js/tree/main/packages/core#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prezly/theme-kit-js.git",
    "directory": "packages/core"
  },
  "publishConfig": {
    "access": "public"
  }
}

{
  "name": "@prezly/theme-kit-nextjs",
  "version": "1.8.0",
  "description": "Data layer and utility library for developing Prezly themes with NextJS",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build",
    "src"
  ],
  "engines": {
    "node": ">= 14.x",
    "npm": ">= 7.x"
  },
  "devEngines": {
    "node": ">= 14.x",
    "npm": ">= 7.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prezly/theme-kit-nextjs.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/prezly/theme-kit-nextjs/issues"
  },
  "homepage": "https://github.com/prezly/theme-kit-nextjs#readme",
  "scripts": {
    "clean": "rimraf build/ node_modules/",
    "build": "tsc --project .",
    "watch": "tsc --watch --preserveWatchOutput --project .",
    "test": "echo \"Tests? maybe next time ;)\" && exit 0",
    "typecheck": "tsc --noEmit --incremental --project .",
    "lint": "eslint src",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "prettier --check './**/*.(js|ts|tsx|css)'",
    "prettier:fix": "npm run prettier -- --write --no-list-different",
    "check": "npm run lint && npm run typecheck",
    "prerelease": "npm run clean && npm install && npm run check && npm run build",
    "release": "np",
    "release:preview": "npm run prerelease && np prerelease --any-branch --no-release-draft --tag=preview"
  },
  "peerDependencies": {
    "next": "^12.x",
    "react": "^17.x",
    "react-dom": "^17.x"
  },
  "dependencies": {
    "@prezly/sdk": "^6.21.0",
    "@prezly/slate-types": "^0.31.1",
    "@react-hookz/web": "^13.2.1",
    "next-seo": "^5.4.0",
    "parse-data-url": "^4.0.1",
    "string-strip-html": "^8.3.0"
  },
  "devDependencies": {
    "@prezly/eslint-config": "^4.0.0",
    "@types/node": "^17.0.13",
    "@types/parse-data-url": "^3.0.0",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "eslint": "^8.8.0",
    "next": "^12.x",
    "np": "^7.6.0",
    "prettier": "^2.5.1",
    "react": "^17.x",
    "react-dom": "^17.x",
    "rimraf": "^3.0.2",
    "typescript": "^4.5.5"
  },
  "publishConfig": {
    "access": "public"
  }
}
